{
  "name": "vite-plugin-string",
  "version": "1.2.1",
  "description": "Converts text files to modules. Such as .vs, .fs, .vert, .frag, .glsl etc.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "unbuild"
  },
  "author": "aweikalee",
  "license": "MIT",
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "vite": "^2.6.14"
  },
  "devDependencies": {
    "@types/estree": "0.0.45",
    "@types/node": "^14.14.19",
    "typescript": "^4.1.2",
    "unbuild": "^1.0.1"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ]
}
